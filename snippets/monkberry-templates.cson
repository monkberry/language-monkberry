'.text.html.monkberry':
  'var':
    'prefix': 'var'
    'body': '{{ $1 }}'
  'for':
    'prefix': 'for'
    'body': '{% for $1 of $2 %}\n\t$3\n{% endfor %}'
  'if':
    'prefix': 'if'
    'body': '{% if $1 %}$0{% endif %}'
  'else':
    'prefix': 'else'
    'body': '{% else %}'    
  'ife':
    'prefix': 'ife'
    'body': '{% if $1 %}$2{% else %}$3{% endif %}'
  'import':
    'prefix': 'import'
    'body': '{% import "$1" %}'
  'unsafe':
    'prefix': 'load'
    'body': '{% unsafe $1 %}'
